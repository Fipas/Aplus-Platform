            sub(ng-if="data.cjzjyikJ === -1") j
            sub(ng-if="data.cjzjyikJ !== -1") {{data.cjzjyikJ}}
            = ' - '
            span c
            sub(ng-if="data.cjzjyikJ === -1") j
            sub(ng-if="data.cjzjyikJ !== -1") {{data.cjzjyikJ}}
            span ) / y
            sup(ng-if="data.cjzjyikI === -1") i
            sup(ng-if="data.cjzjyikI !== -1") {{data.cjzjyikI}}
            sub(ng-if="data.cjzjyikK === -1") k
            sub(ng-if="data.cjzjyikK !== -1") {{data.cjzjyikK}}

        = '  i = '
