            option(value="-1", disabled="disabled", selected)
            option(ng-repeat="j in [info.columns] | makeRange", value="{{j + 1}}") {{j + 1}}
        = '  k = '
        select.form-control.IB-select(ng-model="data.cjzjyikK")
            option(value="-1", disabled="disabled", selected)
            option(ng-repeat="j in [info.columns - info.lines] | makeRange", value="{{j + 1}}") {{j + 1}}

