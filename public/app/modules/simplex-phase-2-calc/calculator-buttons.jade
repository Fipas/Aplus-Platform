.row
    .side-margins.inline-blocker
        .btn-group
            button.btn.btn-info(type="button", ng-if="data.cjzjSwitch === 0", ng-click="calculate('cjzj')")
                span c
                sub(ng-if="data.zjcjJ === -1") j
                sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}
                = ' - '
                span z
                sub(ng-if="data.zjcjJ === -1") j
                sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}
            button.btn.btn-info(type="button", ng-if="data.cjzjSwitch === 1", ng-click="calculate('zjcj')")
                span z
                sub(ng-if="data.zjcjJ === -1") j
                sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}
                = ' - '
                span c
                sub(ng-if="data.zjcjJ === -1") j
                sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}
            button.btn.btn-info.dropdown-toggle(type="button", data-toggle="dropdown")
                span.caret

            ul.dropdown-menu(role="menu")
                li
                    a(ng-click="data.cjzjSwitch = 0")
                        span c
                        sub(ng-if="data.zjcjJ === -1") j
                        sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}
                        = ' - '
                        span z
                        sub(ng-if="data.zjcjJ === -1") j
                        sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}   
                li
                    a(ng-click="data.cjzjSwitch = 1")
                        span z
                        sub(ng-if="data.zjcjJ === -1") j
                        sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}
                        = ' - '
                        span c
                        sub(ng-if="data.zjcjJ === -1") j
                        sub(ng-if="data.zjcjJ !== -1") {{data.zjcjJ}}  

        = '  j = '
        select.form-control.IB-select(ng-model="data.zjcjJ")
            option(value="-1", disabled="disabled", selected)
            option(ng-repeat="j in [info.columns] | makeRange", value="{{j + 1}}") {{j + 1}}

    .side-margins.inline-blocker
        button.btn.btn-info(ng-click="calculate('yi')") 
            span y
            sup(ng-if="data.yiI === -1") i
            sup(ng-if="data.yiI !== -1") {{data.yiI}}
            = ' '
            span = B<sup>-1</sup>
            sub(ng-if="data.yiI === -1") i
            sub(ng-if="data.yiI !== -1") {{data.yiI}}
            = ' N'
        = '  i = '
        select.form-control.IB-select(ng-model="data.yiI")
            option(value="-1", disabled="disabled", selected)
            option(ng-repeat="i in [info.lines] | makeRange", value="{{i + 1}}") {{i + 1}}

    .side-margins.inline-blocker
        button.btn.btn-info(type="button", ng-if="data.cjzjSwitch === 0", ng-click="calculate('cjzjyik')")
            span (c
            sub(ng-if="data.cjzjyikJ === -1") j
            sub(ng-if="data.cjzjyikJ !== -1") {{data.cjzjyikJ}}
