    .side-margins.inline-blocker
        button.btn.btn-info(ng-click="calculate('zj')") 
            span z
            sub(ng-if="data.zjJ === -1") j
            sub(ng-if="data.zjJ !== -1") {{data.zjJ}}
            = ' '
            span = u<sup>T</sup>a
            sub(ng-if="data.zjJ === -1") j
            sub(ng-if="data.zjJ !== -1") {{data.zjJ}}
        = '  j = '
        select.form-control.IB-select(ng-model="data.zjJ")
            option(value="-1", disabled="disabled")
            option(ng-repeat="j in [info.columns] | makeRange", value="{{j + 1}}") {{j + 1}}
    .side-margins.inline-blocker
        button.btn.btn-info(ng-click="calculate('hj')") 
            span h
